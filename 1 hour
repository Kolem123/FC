local correctPasswords = {
    "1"
}

local scriptRuns = 0 -- ĞšÑ–Ğ»ÑŒĞºÑ–ÑÑ‚ÑŒ Ğ·Ğ°Ğ¿ÑƒÑĞºÑ–Ğ² ÑĞºÑ€Ğ¸Ğ¿Ñ‚Ğ°
local passwordEntered = false -- ĞŸÑ€Ğ°Ğ¿Ğ¾Ñ€ĞµÑ†ÑŒ Ğ´Ğ»Ñ Ğ²Ñ–Ğ´ÑÑ‚ĞµĞ¶ĞµĞ½Ğ½Ñ Ğ²Ğ²ĞµĞ´ĞµĞ½Ğ½Ñ Ğ¿Ğ°Ñ€Ğ¾Ğ»Ñ

function InputPassword()
    if not passwordEntered then
        gg.alert("TO ACCESS THE SCRIPT,                             YOU NEED TO ENTER A PASSWORD:")
        local password = gg.prompt({
            "ENTER PASSWORD:"
        }, nil, {
            "text"
        })

        if password == nil then
            gg.alert("FAILED TO ACCESS SCRIPT.")
            os.exit()
        elseif password[1] == "" or not CheckPassword(password[1]) then
            gg.alert("WRONG PASSWORD,STUPID!.                                                    TRY AGAIN.")
            os.exit()
        else
            gg.alert("PASSWORD IS CORRECT,                                   PLEASE ENJOY THE SCRIPT.")
            scriptRuns = scriptRuns + 1
            SaveState(password[1]) -- Ğ—Ğ±ĞµÑ€ĞµĞ¶ĞµĞ½Ğ½Ñ ÑÑ‚Ğ°Ğ½Ñƒ (ĞºÑ–Ğ»ÑŒĞºÑ–ÑÑ‚ÑŒ Ğ·Ğ°Ğ¿ÑƒÑĞºÑ–Ğ²) Ñƒ Ñ„Ğ°Ğ¹Ğ»
            passwordEntered = true -- ĞŸĞ¾Ğ·Ğ½Ğ°Ñ‡ĞµĞ½Ğ½Ñ Ñ‚Ğ¾Ğ³Ğ¾, Ñ‰Ğ¾ Ğ¿Ğ°Ñ€Ğ¾Ğ»ÑŒ Ğ²Ğ²ĞµĞ´ĞµĞ½Ğ¾
        end --SCRIPT by @KolemStoree
    end --SCRIPT by @KolemStoree
end --SCRIPT by @KolemStoree

function CheckPassword(password)
    for _, value in ipairs(correctPasswords) do
        if password == value then
            return true
        end --SCRIPT by @KolemStoree
    end --SCRIPT by @KolemStoree
    return false
end --SCRIPT by @KolemStoree

function SaveState(password)
    local start_time = os.time() -- Ğ—Ğ°Ğ¿Ğ¸ÑÑ‹Ğ²Ğ°ĞµĞ¼ Ğ²Ñ€ĞµĞ¼Ñ Ğ½Ğ°Ñ‡Ğ°Ğ»Ğ° Ñ€Ğ°Ğ±Ğ¾Ñ‚Ñ‹ Ñ„ÑƒĞ½ĞºÑ†Ğ¸Ğ¸
end


        






